{
	"name": "68publishers/user",
	"description": "Provides user management for your Nette project.",
	"keywords": ["68publishers", "user", "management"],
	"license": "MIT",
	"authors": [
		{
			"name": "Vojtěch Lacina",
			"email": "MoraviaD1@gmail.com"
		},
		{
			"name": "Tomáš Glawaty",
			"email": "tomasglawaty@icloud.com"
		}
	],
	"require": {
		"php": "^7.3",
		"68publishers/doctrine-bridge": "^0.1.1",
		"68publishers/doctrine-persistence": "^0.2",
		"68publishers/doctrine-query-objects": "^0.1",
		"68publishers/smart-nette-component": "^0.2",
		"68publishers/translation-bridge": "^1.2",
		"matomo/device-detector": "^4.0",
		"nette/application": "^3.0.6",
		"nette/di": "^3.0.3",
		"nette/forms": "^3.0",
		"nette/http": "^3.0",
		"nette/security": "^3.0",
		"nette/utils": "^3.0",
		"ramsey/uuid": "^4.1",
		"symfony/event-dispatcher": "^5.1",
		"tracy/tracy": "^2.6"
	},
	"require-dev": {
		"roave/security-advisories": "dev-master",
		"friendsofphp/php-cs-fixer": "^2.0",
		"nette/tester": "^2.2",
		"contributte/mailing": "^0.4",
		"psr/log": "^1.0"
	},
	"suggest": {
		"contributte/mailing": "If you want to use default mail sender."
	},
	"autoload": {
		"psr-4": {
			"SixtyEightPublishers\\User\\": "src/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"SixtyEightPublishers\\User\\Tests\\": "tests/"
		}
	},
	"config": {
		"sort-packages": true
	}
}
